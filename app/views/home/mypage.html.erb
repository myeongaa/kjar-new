<% @reservations.each do |r| %>
    <%= Company.find(r.company_id).name %> 
    <%= r.reserve_time %> 
    <%= r.reserve_date %> 
    <%= link_to "자세히 보기",cp_reservation_path(r.company_id,r.id)%></br>
